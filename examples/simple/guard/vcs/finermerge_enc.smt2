(set-logic HORN)
(declare-fun Q_n0 (Int Int Int Int) Bool)
(declare-fun Q_n1 (Int Int Int Int) Bool)
(declare-fun Q_n0_a (Int Int Int Int) Bool)
(declare-fun Q_n0_b (Int Int Int Int) Bool)
(declare-fun Q_n1_a (Int Int Int Int) Bool)
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int)) (=> (and (= xo xa) (= xa xb) (= xb xm)) (Q_n0 xo xa xb xm))))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int)) (=> (Q_n0 xo xa xb xm) true)))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int)) (=> (Q_n0 xo xa xb xm) (Q_n0_a xo xa xb xm))))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int) (xa1 Int) (xm1 Int)) (=> (Q_n0_a xo xa xb xm) true)))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int) (xa1 Int) (xm1 Int)) (=> (and (= xm1 1) (= xa1 1) (Q_n0_a xo xa xb xm)) (Q_n1_a xo xa1 xb xm1))))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int)) (=> (Q_n0_b xo xa xb xm) (and (not (= xb 1)) (not (= xm 1))))))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int)) (=> (and (not (= xm 1)) (not (= xb 1)) (Q_n0_b xo xa xb xm)) (Q_n1 xo xa xb xm))))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int)) (=> (Q_n1_a xo xa xb xm) (and (not (= xa 1)) (not (= xm 1))))))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int)) (=> (and (not (= xm 1)) (not (= xa 1)) (Q_n1_a xo xa xb xm)) (Q_n0_b xo xa xb xm))))
(assert (forall ((xo Int) (xa Int) (xb Int) (xm Int)) (=> (Q_n1 xo xa xb xm) (and (and (or (= xo xa) (= xm xa)) (or (= xo xb) (= xm xb)) (or (not (= xo xa)) (not (= xo xb)) (= xm xo)))))))
(check-sat)
